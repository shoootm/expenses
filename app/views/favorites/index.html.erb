<header class="top_page_header">
    <div class="header_left">
      <i class="fa fa-home"></i>
      お気に入り一覧
    </div>
</header>
<div class="main_container">
<div class="expense_lists">
<% unless @expenses == [] %>
<% @favorites.each do |favorite| %>
  <%= render partial: "expenses/expense", locals: { expense: favorite.expense } %>
  お気に入り
  <% if favorite.expense.favorite_counts.count >= 1 %>
    <%= favorite.expense.favorite_counts[0]['count'] %>
  <% end %>
<% end %>
<% end %>
</div>
</div>

<%= link_to "トップページ", root_path %>